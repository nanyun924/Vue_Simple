{
  "name": "vue-testing-examples",
  "version": "0.0.1-0",
  "private": true,
  "description": "Advanced testing with vuejs. When you need to go beyond Getting started section and see some real world example with everything that proper tests should have.",
  "license": "MIT",
  "author": "mcibique (https://github.com/mcibique)",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "nyc vue-cli-service test:unit src/**/*.spec.js src/router.spec.js src/store/**/*.spec.js src/App.vue.spec.js --include ./src/bootstrap.js ./test/unit/index.js",
    "test:watch": "npm run test -- --watch",
    "lint": "vue-cli-service lint"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "axios": "0.18.0",
    "babel-polyfill": "6.26.0",
    "inversify": "4.13.0",
    "inversify-inject-decorators": "3.1.0",
    "inversify-vanillajs-helpers": "2.0.0",
    "lodash": "4.17.10",
    "reflect-metadata": "0.1.12",
    "vue": "2.5.17",
    "vue-property-decorator": "7.0.0",
    "vue-router": "3.0.1",
    "vuex": "3.0.1",
    "vuex-class": "0.3.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.0.1",
    "@vue/cli-plugin-eslint": "3.0.1",
    "@vue/cli-plugin-unit-mocha": "3.0.1",
    "@vue/cli-service": "3.0.1",
    "@vue/eslint-config-standard": "3.0.1",
    "@vue/test-utils": "1.0.0-beta.24",
    "axios-mock-adapter": "1.15.0",
    "babel-plugin-istanbul": "5.0.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "flush-promises": "1.0.0",
    "husky": "0.14.3",
    "lint-staged": "7.2.2",
    "node-sass": "4.9.3",
    "nyc": "13.0.1",
    "sass-loader": "7.1.0",
    "sinon": "4.5.0",
    "sinon-chai": "3.2.0",
    "sinon-stub-promise": "4.0.0",
    "vue-template-compiler": "2.5.17"
  }
}
